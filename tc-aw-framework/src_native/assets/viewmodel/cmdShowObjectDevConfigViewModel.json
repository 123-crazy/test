        ],
        "UseNewOccTitle": [
            "ShowObjectDevConfigMessages"
        ],
        "UseDeclShowObjTitle": [
            "ShowObjectDevConfigMessages"
        ],
        "DelayTimeTreeTitle": [
            "ShowObjectDevConfigMessages"
        ],
        "DelayTimePropertyTitle": [
            "ShowObjectDevConfigMessages"
        ],
        "MaxTreeLevelTitle": [
            "ShowObjectDevConfigMessages"
        ],
        "InitialChildNdxTitle": [
            "ShowObjectDevConfigMessages"
        ],
        "LoadPageSizeTitle": [
            "ShowObjectDevConfigMessages"
        ]
    },
    "data": {
        "useMockData": {
            "displayName": "{{i18n.UseMockDataTitle}}",
            "type": "BOOLEAN",
            "isRequired": "false",
            "isEditable": "true",
            "dbValue": "{{ctx.showObjectDevConfig.useMock}}",
            "dispValue": "",
            "labelPosition": "PROPERTY_LABEL_AT_SIDE"
