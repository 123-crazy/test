                        }
                    },
                    {
                        "name": "complete.subPanel",
                        "eventData": {
                            "source": "addReferenceSub"
                        }
                    }
                ]
            }
        }
    },
    "i18n": {
        "ADD_BUTTON_TITLE": [
            "BaseMessages"
        ]
    }
