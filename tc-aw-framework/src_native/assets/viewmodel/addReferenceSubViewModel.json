{
    "schemaVersion": "1.0.0",
    "imports": [
        "js/aw-add.directive",
        "js/aw-panel-footer.directive",
        "js/aw-button.directive",
        "js/aw-i18n.directive"
    ],
    "data": {},
    "actions": {
        "setTypeReference": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
