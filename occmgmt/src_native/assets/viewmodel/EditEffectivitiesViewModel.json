{
    "schemaVersion": "1.0.0",
    "imports":
    [





        "js/aw-include.directive"

    ],

    "i18n":
    {
        "dateEffectivity":
        [
            "OccurrenceManagementConstants"
        ],

        "dateEffectivityPlaceholder":
        [
            "OccurrenceManagementConstants"
        ],

        "duplicateEffectivityCreationErrorMessage":
        [
            "OccurrenceManagementMessages"
        ],

        "effectivityType":
        [
            "OccurrenceManagementConstants"
        ],

        "endDate":
        [
            "OccurrenceManagementConstants"
        ],

        "endItemMessage":
        [
            "OccurrenceManagementConstants"
        ],

        "endItemRequired":
        [
            "OccurrenceManagementConstants"
        ],

        "name":
        [
            "OccurrenceManagementConstants"
        ],

        "protect":
        [
            "OccurrenceManagementConstants"
        ],

        "aceRemove":
        [
            "OccurrenceManagementConstants"
        ],

        "saveButtonText":
        [
            "OccurrenceManagementConstants"
        ],

        "sharedEffectivities":
        [
            "OccurrenceManagementConstants"
        ],

        "shareEffectivity":
        [
            "OccurrenceManagementConstants"
        ],

        "soText":
        [
            "OccurrenceManagementConstants"
        ],

        "startDate":
        [
            "OccurrenceManagementConstants"
        ],

        "unitEffectivity":
        [
            "OccurrenceManagementConstants"
        ],

        "upText":
        [
            "OccurrenceManagementConstants"
        ]
    },

    "data":
    {
        "flag": {
            "type": "STRING",
            "dbValue": "EDIT"
        },
        "nameBox":
        {
            "displayName": "{{i18n.name}}",
            "type": "STRING",
            "dbValue":"{{ctx.editEffectivityContext.props.nameBox}}",
            "isRequired": "true",
            "maxLength": 32
        },

        "unitRangeText":
        {
            "displayName": "{{i18n.unitEffectivity}}",
            "type": "STRING",
            "dbValue":"{{ctx.editEffectivityContext.props.unitRangeText}}",
            "isRequired": "true"
        },
        "isShared":
        {
            "displayName": "{{i18n.shareEffectivity}}",
            "type": "BOOLEAN",
            "isRequired": "true",
            "isEditable": "true",
            "dbValue":"{{ctx.editEffectivityContext.props.isShared}}",
            "labelPosition": "PROPERTY_LABEL_AT_RIGHT"
        },

        "dateOrUnitEffectivityTypeRadioButton":
        {
            "type": "BOOLEAN",
            "isRequired": "true",
            "propertyRadioTrueText": "{{i18n.dateEffectivity}}",
            "propertyRadioFalseText": "{{i18n.unitEffectivity}}",
            "dbValue":"{{ctx.editEffectivityContext.props.dateOrUnitEffectivityTypeRadioButton}}",
            "labelPosition": "PROPERTY_LABEL_AT_RIGHT"
        },

        "startDate":
        {
            "dbValue":"{{ctx.editEffectivityContext.props.startDate}}",
            "type": "DATE",
            "isRequired": "true",
            "displayName": "{{i18n.startDate}}",
            "isEditable": "true"
        },

        "endDate":
        {
            "dbValue":"{{ctx.editEffectivityContext.props.endDate}}",
            "type": "DATE",
            "isRequired": "true"
        },
        "endDateOptions":
        {
            "type": "STRING",
            "isRequired": "true",
            "hasLov": true,
            "displayName": "{{i18n.endDate}}",
            "dbValue":"{{ctx.editEffectivityContext.props.endDateOption}}",
            "isEditable": "true"
        },
        "endItemVal":
        {
            "type": "STRING",
            "uiValue":"{{ctx.editEffectivityContext.props.endItemVal}}",
            "displayName": "{{i18n.endItemMessage}}"
        },
        "isProtected":{
            "displayName": "{{i18n.protect}}",
            "type": "BOOLEAN",
            "isRequired": "false",
            "isEditable": "true",
            "dbValue":"{{ctx.editEffectivityContext.props.isProtected}}",
            "labelPosition":"PROPERTY_LABEL_AT_RIGHT"
        }
    },
    "actions":
    {
    },

