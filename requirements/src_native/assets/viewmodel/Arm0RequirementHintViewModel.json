{
    "schemaVersion": "1.0.0",
    "imports":
    [
        "js/aw-icon-button.directive"
    ],

    "actions":
    {
        "createDropTracelinkHintLabel": {
            "actionType": "JSFunction",
            "method": "createDropTracelinkHintLabel",
            "inputData": {
                "data": "{{data}}"
            },
            "deps": "js/Arm0CreateTraceLinkPopupService"
        }
    },

    "data":
    {
        "buttonCommand": {
            "iconName": "cmdCreateTraceLink",
            "tooltip": "{{i18n.createTraceLinkTitle}}"
        }
    },

    "dataProviders":
    {},

    "onEvent":
    [
        {
            "eventId": "Arm0RequirementHint.contentLoaded",
            "action": "createDropTracelinkHintLabel"
        }
    ],

    "conditions":
    {},

    "i18n":
    {
        "dropTracelinkHintLabel": [
            "RequirementsCommandPanelsMessages"
        ],
        "createTraceLinkTitle":[
            "RequirementsCommandPanelsMessages"
        ]
    }
}
