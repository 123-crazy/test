        },

        "useTransparencyCheckboxProp": {
            "displayName": "{{i18n.useTransparencyCheckboxName}}",
            "type": "BOOLEAN",
            "isRequired": "false",
            "isEditable": "true",
            "dbValue": "{{ctx.viewer.preference.AWC_visSelectionDisplay}}",
            "dispValue": "",
            "labelPosition": "PROPERTY_LABEL_AT_RIGHT"
        },

        "floorSliderProp": {
            "isArray": "true",
            "dbValue": [ {
                "sliderOption": {
                    "disabled": false,
                    "min": 0,
                    "max": 100,
                    "step": 1,
                    "value": 50,
                    "orientation": "horizontal",
                    "range": false
                },

                "showIncrementButtons": true,
                "sliderChangeEventAction": "handleSliderChangeEvent"
            } ]
        }
    },

    "messages": {
        "confirmRenderSourceChangeServer": {
            "messageType": "WARNING",
            "messageText": "{{i18n.renderSourceWarningServerText}}",
            "messageTextParams": [
                "{{data.modelName.propertyDisplayName}}"
            ],
            "navigationOptions": [ {
                    "option": "Cancel",
                    "text": "{{i18n.cancel}}",
                    "action": "renderSourceChangeCancelled"
                },
                {
                    "option": "Reload",
                    "text": "{{i18n.reload}}",
                    "action": "renderSourceChangeSuccess"
                }
            ]
        },
        "confirmRenderSourceChangeClient": {
            "messageType": "WARNING",
            "messageText": "{{i18n.renderSourceWarningClientText}}",
            "messageTextParams": [
                "{{data.modelName.propertyDisplayName}}"
            ],
            "navigationOptions": [ {
                    "option": "Cancel",
                    "text": "{{i18n.cancel}}",
                    "action": "renderSourceChangeCancelled"
                },
                {
                    "option": "Reload",
                    "text": "{{i18n.reload}}",
                    "action": "renderSourceChangeSuccess"
                }
            ]
        }
    },

    "conditions": {
        "showFloorOptions": {
            "expression": "data.showFloorCheckboxProp.dbValue == true"
        },

        "showShadedWithEdges": {
            "expression": "data.shadedCheckboxProp.isVisible == true"
        },

        "showPMI": {
            "expression": "data.pmiCheckboxProp.isVisible == true"
        },

        "walkModeAvailability": {
            "expression": "data.isWalkModeAvailability"
        },

        "viewerLicenceOnOrAboveStandard": {
            "expression": "ctx.viewer && ctx.viewer.licLevel && ctx.viewer.licLevel>=1"
        },

        "viewerLicenceOnOrAboveProfessional": {
            "expression": "ctx.viewer && ctx.viewer.licLevel && ctx.viewer.licLevel>=2"
        },

        "isNonPVWCSRViewer":{
            "expression":"ctx.viewer && ctx.viewer.preference && (ctx.viewer.preference.AWV0ViewerRenderOption[0] === 'SSR' || ctx.viewer.preference.AWV0ViewerRenderOption[0] === 'CSR' && ctx.viewer.preference.AWV0ViewerCSROption !== 'PVW')"
        },

        "showIndexedOption": {
            "expression": "data.useIndexedCheckboxProp.isVisible == true"
        },

        "showRenderOptions": {
            "expression": "data.renderSourceServerCheckboxProp.dbValue !== null"
        }

    },

