            "displayName": "{{i18n.shadedFeatureLine}}",
            "type": "BOOLEAN",
            "isRequired": "false",
            "isVisible": "false",
            "isEditable": "true",
            "dbValue": "{{ctx.viewer.preference.AWC_visShading}}",
            "dispValue": "",
            "labelPosition": "PROPERTY_LABEL_AT_RIGHT"
        },

        "materialCheckboxProp": {
            "displayName": "",
            "type": "BOOLEAN",
            "isRequired": "false",
            "isEditable": "true",
            "dbValue": "{{ctx.viewer.preference.AWC_applyTrueShadingMaterial}}",
            "dispValue": "",
            "labelPosition": "PROPERTY_LABEL_AT_RIGHT"
        },

        "materialCommandProp": {
            "action": "showMaterialPopupEvent",
            "iconName": "",
            "tooltip": ""
        },

        "materialProp": {
            "displayName": "",
            "isArray": "true",
            "dbValue": [ {
                "iconName": "",
                "tooltip": "",
                "checkAction": "materialSettingChanged",
                "buttonAction": "showMaterialPopupEvent",
                "isChecked": "{{ctx.viewer.preference.AWC_applyTrueShadingMaterial}}",
                "materialIndex": "{{ctx.viewer.preference.AWC_visMaterial}}"
            } ]
        },

