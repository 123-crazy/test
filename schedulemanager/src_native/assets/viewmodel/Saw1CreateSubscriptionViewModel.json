{
    "schemaVersion": "1.0.0",

    "imports": [
        "js/aw-command-panel.directive",
        "js/aw-command-panel-section.directive",
        "js/aw-command-sub-panel.directive"
    ],

    "actions": {
        "saw1SetPin": {
            "actionType": "JSFunction",
            "method": "setPin",
            "inputData": {
                "data": "{{data.eventData.data}}"
            },
            "outputData": {},
            "deps": "js/addObjectUtils"
        },
        "saw1SetUnPin": {
            "actionType": "JSFunction",
            "method": "setUnPin",
            "inputData": {
                "data": "{{data.eventData.data}}"
            },
            "outputData": {},
            "deps": "js/addObjectUtils"
        }
    },

    "data": {
        "Saw1PinCommandsCreatePanel": "saw1_oneStepPinCommands",
        "pinnedToForm": {
            "type": "BOOLEAN",
            "dbValue": true
        },
        "unpinnedToForm": {
            "type": "BOOLEAN",
            "dbValue": false
        }
    },

    "i18n": {
        "followPanelOrCommandTitle": [
            "ScheduleManagerMessages"
        ]
    }
}
