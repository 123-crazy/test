    "conditions": {
      "defineLogicalObjectVisible": {
        "expression": "ctx.logicalObject"
      },
      "searchLogicalObjectVisible": {
        "expression": "ctx.logicalObject"
      },
      "canAddMemberOrProp": {
        "expression": "!ctx.editInProgress && ctx.selected && ctx.selected.props.fnd0RootTypeName && commandContext.dataProvider.selectionModel.getCurrentSelectedCount()===0"
      },
      "canEditMember": {
        "expression": "!ctx.editInProgress && ctx.selected && !ctx.selected.props.fnd0Inherited.dbValue && commandContext.dataProvider.selectionModel.getCurrentSelectedCount()===1"
      },
      "addMemberCommandVisible": {
        "expression": "conditions.canAddMemberOrProp && commandContext.objectSetSource==='fnd0Members.Fnd0LOPropPresentations'"
      },
      "editMemberCommandVisible": {
        "expression": "conditions.canEditMember && commandContext.objectSetSource==='fnd0Members.Fnd0LOPropPresentations'"
      },
      "addPropertyCommandVisible": {
        "expression": "conditions.canAddMemberOrProp && commandContext.objectSetSource==='fnd0PresentedProperties.Fnd0LOPropPresentations'"
      },
      "addIncludedLogicalObjectCommandVisible": {
        "expression": "conditions.canAddMemberOrProp"
      },
      "deleteLogicalObjectVisible": {
        "expression": "ctx.visibleServerCommands.LO1DeleteLogicalObject && !ctx.selected.props.fnd0TargetPresentedPropName"
      },
      "isAwp0RootAddCmdVisible": {
        "expression": "commandContext.rootSet.length == 0"
      },
      "isAwp0RootRemoveCmdVisible": {
        "expression": "commandContext.dataProviders.rootSetProvider.selectedObjects.length > 0"
      },
      "isAwp0ParentAddCmdVisible": {
        "expression": "commandContext.parentSet.length == 0"
      },
      "isAwp0ParentRemoveCmdVisible": {
        "expression": "commandContext.dataProviders.parentSetProvider.selectedObjects.length > 0"
      }
