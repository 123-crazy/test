      "Qam0PasteOperationQualityAction": {
        "actionType": "JSFunction",
        "method": "Qam0SoaInputPasteOperationQualityAction",
        "inputData": {
          "ctx": "{{ctx}}",
          "data": "{{data}}",
          "location": "qam0DependentQualityActions"
        },
        "outputData": {
          "input": "input",
          "pasteFailOtherObject": "pasteFailOtherObject",
          "pasteFailSelfDepedent": "pasteFailSelfDepedent",
          "existQualityAction": "existQualityAction",
          "pasteSuccessObjects": "pasteSuccessObjects",
          "elementExist": "elementExist"
        },
        "events": {
          "success": [
            {
              "condition": "data.input.length !== 0",
              "name": "Qam0PerfromPasteOperationQualityAction"
            }
          ]
        },
        "actionMessages": {
          "success": [
            {
              "condition": "data.pasteFailSelfDepedent.length === 1",
              "message": "PasteFailForSelfDependentErrorMsg"
            },
            {
              "condition": "data.existQualityAction.length === 1",
              "message": "pasteFailForExistDependentQualityAction"
            }
          ]
        },
        "deps": "js/Qam0QualityActionService"
      },
      "Qam0PasteOperationQualityActionInTarget": {
        "actionType": "JSFunction",
        "method": "Qam0SoaInputPasteOperationQualityAction",
        "inputData": {
          "ctx": "{{ctx}}",
          "data": "{{data}}",
          "location": "qam0Targets"
        },
        "outputData": {
          "input": "input",
          "pasteFailOtherObject": "pasteFailOtherObject",
          "pasteFailSelfDepedent": "pasteFailSelfDepedent",
          "existQualityAction": "existQualityAction",
          "pasteSuccessObjects": "pasteSuccessObjects",
          "elementExist": "elementExist"
        },
        "events": {
          "success": [
            {
              "condition": "data.input.length !== 0",
              "name": "Qam0PerfromPasteOperationQualityAction"
            }
          ]
        },
        "actionMessages": {
          "success": [
            {
              "condition": "data.pasteFailSelfDepedent.length === 1",
              "message": "PasteFailForSelfDependentErrorMsg"
            },
            {
              "condition": "data.existQualityAction.length === 1",
              "message": "pasteFailForExistDependentQualityAction"
            }
          ]
        },
        "deps": "js/Qam0QualityActionService"
      },
      "Qam0PerfromPasteOperationQualityAction": {
        "actionType": "TcSoaService",
        "serviceName": "Core-2006-03-DataManagement",
        "method": "createRelations",
        "inputData": {
          "input": "{{data.input}}"
        },
        "events": {
          "success": [
            {
              "name": "cdm.relatedModified",
              "refreshLocationFlag": true,
              "eventData": {
                "relatedModified": [
                  "{{ctx.selected}}"
                ]
              }
            }
          ]
        },
        "actionMessages": {
          "success": [
            {
              "condition": "data.input.length === 1 &&  !(ctx.xrtSummaryContextObject.modelType.typeHierarchyArray.indexOf('Qam0QualityAction') > -1) ",
              "message": "pasteSuccessMessage"
            },
            {
              "condition": "data.input.length > 1 &&  !(ctx.xrtSummaryContextObject.modelType.typeHierarchyArray.indexOf('Qam0QualityAction') > -1)",
              "message": "pasteMultipleSuccessMessage"
            }
          ]
        },
        "deps": "js/Qam0QualityActionService"
      },
      "getConfirmationForDeleteQualityAction": {
        "actionType": "Event",
        "method": "Event",
        "inputData": {
          "events": [
            {}
          ]
        },
        "actionMessages": {
          "success": [
            {
              "condition": "ctx.mselected.length == 1",
              "message": "deleteSingleQualityActionMessage"
            },
            {
              "condition": "ctx.mselected.length > 1",
              "message": "deleteMultipleQualityActionMessage"
            }
          ]
        }
      },
      "deleteQualityActionObjects": {
        "actionType": "TcSoaService",
        "serviceName": "Core-2006-03-DataManagement",
        "method": "deleteObjects",
        "inputData": {
          "objects": "{{function:getSelectedObjects}}"
        },
        "events": {
          "success": [
            {
              "name": "cdm.relatedModified",
              "eventData": {
                "relatedModified": [
                  "{{ctx.xrtSummaryContextObject}}"
                ]
              }
            },
            {
              "condition": "ctx.sublocation.historyNameToken === 'QualityActionSubLocation' || ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.qualityCenterManager.QALocation'",
              "name": "primaryWorkarea.reset"
            },
            {
              "condition": "ctx.locationContext.modelObject.type === 'Fnd0HomeFolder'",
              "name": "cdm.relatedModified",
              "eventData": {
                "refreshLocationFlag": true,
                "relatedModified": [
                  "{{ctx.locationContext.modelObject}}"
                ]
              }
            }
          ],
          "failure": [
            {
              "name": "cdm.relatedModified",
              "eventData": {
                "relatedModified": [
                  "{{ctx.xrtSummaryContextObject}}"
                ]
              }
            }
          ]
        },
        "actionMessages": {
          "failure": [
            {
              "message": "SOAFailureMsg"
            }
          ]
        },
        "deps": "js/userListService"
      },
