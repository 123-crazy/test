        "actionType": "TcSoaService",
        "serviceName": "Core-2014-10-DataManagement",
        "method": "removeChildren",
        "inputData": {
          "inputData": [
            {
              "clientId": "",
              "childrenObj": "{{function:getExecutionReferenceObject}}",
              "parentObj": "{{ctx.locationContext.modelObject}}",
              "propertyName": "qam0ExecutionReference"
            }
