{
    "schemaVersion": "1.0.0",
    "imports": [
        "js/aw-panel.directive",
        "js/aw-panel-body.directive",
        "js/aw-button.directive",
        "js/aw-message-params.directive"
    ],
    "data": {
    },
    "actions":
    {
        "buttonShowOutgoing": {
            "actionType":"JSFunction",
            "method": "networkGraphclosePopup",
            "inputData":
            {
                "graphModel": "{{ctx.graph.graphModel}}",
                "node": "{{ctx.graph.commandContextItem}}",
                "direction": "Outgoing",
                "action": "Show"
            },
            "deps": "js/NetworkGraphService"
        },
        "buttonHideOutgoing": {
            "actionType":"JSFunction",
            "method": "networkGraphclosePopup",
            "inputData":
            {
                "graphModel": "{{ctx.graph.graphModel}}",
                "node": "{{ctx.graph.commandContextItem}}",
                "direction": "Outgoing",
                "action": "Hide"
            },
            "deps": "js/NetworkGraphService"
        }
    },
    "i18n": {
        
        "hidePartialOutgoingRelations":
        [
            "ProgramdiagrammingMessages"
        ],
        "showAllOutgoingRelations": 
        [
            "ProgramdiagrammingMessages"
        ]
    }
} 